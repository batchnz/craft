{#-- Global Header --#}
<link rel="dns-prefetch" href="{{ siteUrl }}">
<link rel="preconnect" href="{{ siteUrl }}" crossorigin>

{# -- Misc header tags -- #}
<meta charset="utf-8">
<meta name="viewport" content="width=device-width,minimum-scale=1">

{#-- Favicon --#}
<link rel="apple-touch-icon" sizes="180x180" href="{{ siteUrl }}assets/images/favicon/apple-touch-icon.png">
<link rel="icon" type="image/png" sizes="32x32" href="{{ siteUrl }}assets/images/favicon/favicon-32x32.png">
<link rel="icon" type="image/png" sizes="16x16" href="{{ siteUrl }}assets/images/favicon/favicon-16x16.png">
<link rel="manifest" href="{{ siteUrl }}assets/images/favicon/site.webmanifest">
<link rel="mask-icon" href="{{ siteUrl }}assets/images/favicon/safari-pinned-tab.svg" color="#5bbad5">
<meta name="msapplication-TileColor" content="#da532c">
<meta name="theme-color" content="#ffffff">

{#-- CSS/Links --#}
{{ craft.twigpack.includeCssModule('styles.css', false) }}
{{ craft.twigpack.includeCssRelPreloadPolyfill() }}
