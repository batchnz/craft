{% cache %}
{% set contentSlices = entry.contentBuilder.all() ?? null %}
{% if contentSlices|length %}
    {% for slice in contentSlices %}
        {% switch slice.type %}

            {% case default %}
            {% include ('_slices/mySlice.twig') with { slice: slice } %}

        {% endswitch %}
    {% endfor %}
{% endif %}
{% endcache %}
