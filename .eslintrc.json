{
  "root": true,
  "parser": "vue-eslint-parser",
  "parserOptions": {
    "parser": "babel-eslint",
    "sourceType": "module",
    "ecmaVersion": 2018
  },
  "extends": [
    "plugin:vue/recommended",
    "airbnb-base",
    "prettier/vue",
    "plugin:prettier/recommended"
  ],
  "env": {
    "node": true,
    "browser": true
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["src"]
      },
      "webpack": {
        "config": "node_modules/@batch/craft-webpack/webpack.dev.js"
      }
    }
  }
}
