{
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/batchnz/craft-twigpack"
    }
  ],
  "require": {
    "aelvan/imager": "v2.4.0",
    "craftcms/cms": "3.4.14",
    "craftcms/mailgun": "1.4.2",
    "craftcms/redactor": "2.6.1",
    "nystudio107/craft-seomatic": "3.2.51",
    "nystudio107/craft-twigpack": "1.3.0",
    "solspace/craft-freeform": "3.6.6",
    "spicyweb/craft-neo": "2.7.5",
    "superbig/craft3-http2serverpush": "1.0.1",
    "thejoshsmith/craft-title-to-sidebar": "1.0.1",
    "verbb/image-resizer": "2.0.6",
    "verbb/navigation": "1.3.8",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
